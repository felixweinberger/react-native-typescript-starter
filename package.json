{
  "name": "react-native-typescript-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./",
